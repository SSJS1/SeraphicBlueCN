msgid ""
"モーガン：\n"
"\\S[5]お…の…れ…！\\|\\^"
msgstr ""

#. ID 4, Page 1, Line 30, Pos (0,0)
msgid ""
"モーガン：\n"
"\\S[2]クッ…\\.\\.ククッ…。\\|\\^"
msgstr ""

#. ID 4, Page 1, Line 35, Pos (0,0)
msgid ""
"モーガン：\n"
"\\S[5]道連れだァ…！\\|\\^"
msgstr ""

#. ID 4, Page 1, Line 37, Pos (0,0)
msgid ""
"ユアン：\n"
"何ッ!?\\|\\^"
msgstr ""

#. ID 4, Page 1, Line 46, Pos (0,0)
msgid ""
"モーガン：\n"
"\\S[2]もう逃げられんぞ…。\\|\\.\\.\\^"
msgstr ""

#. ID 4, Page 1, Line 56, Pos (0,0)
msgid ""
"モーガン：\n"
"\\S[2]後ろを見てみろ…。\\|\\.\\.\\^"
msgstr ""

